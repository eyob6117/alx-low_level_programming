#include "variadic_functions.h"
#include <stdarg.h>
/**
 * sum_thme_all - function that returns the sum of all its parameters
 * @n: first parameter
 *
 * Return: to the function
 */
int sum_them_all(const unsigned int n, ...)
{
int i, sum = 0;
va_list ap;
va_start(ap, count);
for (i = 0; i < n; i++)
sum += va_arg(ap, int);
 va_end(ap);ap
